{
  "name": "Example PWA - learn about PWAs",
  "short_name": "Example PWA",
  "start_url": "/index.html",
  "display": "standalone",
  "background_color": "grey",
  "theme_color": "grey",
  "icons": [
    {
      "src": "public/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "public/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "shortcuts": [
    {
      "name": "APIs anzeigen",
      "description": "APIs in der App anzeigen",
      "url": "index.html#apis",
      "icons": [
        {
          "src": "public/shortcut-icon-96x96.png",
          "sizes": "96x96",
          "type": "image/png",
          "purpose": "any"
        }
      ]
    },
    {
      "name": "Akkustand anzeigen",
      "description": "Akkustand in der App anzeigen",
      "url": "/battery.html",
      "icons": [
        {
          "src": "public/shortcut-icon-96x96.png",
          "sizes": "96x96",
          "type": "image/png",
          "purpose": "any"
        }
      ]
    }
  ],
  "file_handlers": [
    {
      "action": "/image.html",
      "accept": {
        "image/jpeg": [
          ".jpg",
          ".jpeg"
        ],
        "image/png": [
          ".png"
        ]
      }
    }
  ]
}
